<h3 id="document-list-heading" class="accessible-hidden">List of files deposited in <%= t('sufia.product_name') %> that match your search criteria</h3>
<ul class="container-fluid search-results-list" aria-labeled-by="document-list-heading">
  <% docs.each_with_index do |document,counter| %>
    <%= render partial: '/catalog/_index_partials/list_files', :locals => {:document => document, :counter => counter} %>
  <% end %>
</ul>
